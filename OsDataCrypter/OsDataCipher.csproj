<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>$(MSBuildProjectDirectory)\..\OsDataCipher.xml</DocumentationFile>
		<DocumentationMarkdown>$(MSBuildProjectDirectory)\..\OsDataCipher.md</DocumentationMarkdown>
		<VsxmdAutoDeleteXml>False</VsxmdAutoDeleteXml>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="Vsxmd" Version="1.4.5">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

</Project>
